	  <dependency id="Jurassic" version="2.1.1" />
    </dependencies>
  </metadata>
  <files>
    <file src="SquishIt.Framework\bin\Release\SquishIt.Framework.dll" target="lib" />
	<file src="SquishIt.Mvc\bin\Release\SquishIt.Mvc.dll" target="optional" />
  </files>
</package>
